<mappings xmlns="http://dozer.sourceforge.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net  http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<date-format>dd/MM/yyyy HH:mm</date-format>
		<trim-strings>true</trim-strings>
	</configuration>

	<mapping map-id="MachineAllotMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.allotment.MachineAllotmentEntity
		</class-a>
		<class-b>com.hitech.dms.web.model.allot.create.request.MachineAllotCreateRequestModel
		</class-b>
	</mapping>
	
	<mapping map-id="DCMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.delivery.DeliveryChallanHdrEntity
		</class-a>
		<class-b>com.hitech.dms.web.model.dc.create.request.DcCreateRequestModel
		</class-b>
	</mapping>
	
	<mapping map-id="InvoiceMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.invoice.MachineInvoiceHDREntity
		</class-a>
		<class-b>com.hitech.dms.web.model.inv.create.request.InvoiceCreateRequestModel
		</class-b>
	</mapping>
	
	
	<mapping map-id="partyCodeMasterMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.model.partybybranch.create.request.PartyCodeCreateRequestModel
		</class-a>
		<class-b>com.hitech.dms.web.entity.partycode.PartyCodeEntity
		</class-b>
	</mapping>
	

</mappings>                                                                                                              
                                                                                                                         
                                                                                                                         
	                                                                                                                     
                                                                                                                         
                                                                                                                         
                                                                                                                         