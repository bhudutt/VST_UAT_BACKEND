<mappings xmlns="http://dozer.sourceforge.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net  http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<date-format>dd/MM/yyyy HH:mm</date-format>
		<trim-strings>true</trim-strings>
	</configuration>

	<mapping map-id="ActivityPlanHdrMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.activityPlan.ActivityPlanHDREntity
		</class-a>
		<class-b>com.hitech.dms.web.entity.activityPlan.stg.StgActivityPlanHDREntity
		</class-b>
	</mapping>
	
	<mapping map-id="ActivityPlanDLrMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.activityPlan.ActivityPlanDLRModelEntity
		</class-a>
		<class-b>com.hitech.dms.web.entity.activityPlan.stg.StgActivityPlanDLRModelEntity
		</class-b>
		<field-exclude>
			<a>activityPlanDlrId</a>
			<b>activityPlanDlrId</b>
		</field-exclude>
	</mapping>
	
	<mapping map-id="ActivityPlanDLrDtlMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.activityPlan.ActivityPlanDLRModelDTLEntity
		</class-a>
		<class-b>com.hitech.dms.web.entity.activityPlan.stg.StgActivityPlanDLRModelDTLEntity
		</class-b>
		<field-exclude>
			<a>activityPlanDlrDtlId</a>
			<b>activityPlanDlrDtlId</b>
		</field-exclude>
	</mapping>
	
	<mapping map-id="ActualActivityCreateMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.activity.ActualActivityHDREntity
		</class-a>
		<class-b>com.hitech.dms.web.model.activity.create.request.ActualActivityRequestModel
		</class-b>
	</mapping>
	
	<mapping map-id="ActivityNoListResponseMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.model.activityClaim.create.response.ActivityClaimPlanResponseModel
		</class-a>
		<class-b>com.hitech.dms.web.model.activityClaim.approval.response.ActivityClaimDtlListResponseModel
		</class-b>
	</mapping>
	
	<mapping map-id="ActivityClaimHdrResMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.model.activityClaim.create.response.ActivityClaimHdrResponseModel
		</class-a>
		<class-b>com.hitech.dms.web.model.activityClaim.invoice.response.ActivityClaimInvHdrResponseModel
		</class-b>
	</mapping>
	
	<mapping map-id="ActivityClaimCreateResMapId" type="bi-directional">
		<class-a>com.hitech.dms.web.entity.activityClaim.invoice.ActivityClaimInvoiceHdrEntity
		</class-a>
		<class-b>com.hitech.dms.web.model.activityClaim.invoice.create.request.ActivityClaimInvCreateRequestModel
		</class-b>
	</mapping>

	<!-- <mapping map-id="EnquiryImgMapId" type="bi-directional" wildcard="false">
